						<tr>
							<td>All flights</td>

							<td></td>
						</tr>

						<tr>
							<td>Flights in 2020</td>

							<td></td>
						</tr>

						<tr>
							<td>Flights in 2019</td>

							<td></td>
						</tr>

						<tr>
							<td>Flights in 2018</td>

							<td></td>
						</tr>

						<tr>
							<td>Flights inside</td>

							<td></td>
						</tr>

						<tr>
							<td>Flights outside</td>

							<td></td>
						</tr>

					</tbody>
				</table>
			</div>
			<div class="col">
				<h2>Flights by drones</h2>
				<table class="table table-borderless">
					<thead>
						<tr>
							<th>Drone name</th>
							<th>total flights</th>
						</tr>
					</thead>
					<tbody>
						<tr>

							<td></td>
							<td></td>
						</tr>

					</tbody>
				</table>
			</div>
		</div>

